  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polidea/react-native-ble-plx.git"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "22.2.0",
    "babel-preset-react-native": "^5.0.2",
    "documentation": "^9.1.1",
    "eslint": "^5.12.1",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.7.0",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "^0.63.0",
    "jest": "22.2.1",
    "prettier": "1.10.2",
    "react": "16.2.0",
    "react-native": "^0.57.8"
  },
  "keywords": [
    "React",
    "Native",
    "Bluetooth",
    "Low",
    "Energy",
    "BLE",
